<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
  "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>pl.loee.ssh-agent</string>

    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/loee-agent</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardErrorPath</key>
    <string>/tmp/pl.loee.ssh-agent.err</string>

    <key>StandardOutPath</key>
    <string>/tmp/pl.loee.ssh-agent.out</string>
</dict>
</plist>
